<?xml version="1.0" encoding="UTF-8"?>
<dao-class xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="dao.xsd">

    <crud dto="DownloadsInfo" table="downloads">
        <create method="create_download"/>
        <update method="update_download"/>
    </crud>

    <query-dto-list method="find(r_id, p_date)" dto="DownloadsInfo" ref="find_downloads.sql"/>

    <query-dto-list method="get_latest(int r_id, int start, int count)" dto="DownloadsInfo" ref="get_latest.sql"/>

</dao-class>